# This tmux statusbar config was created by tmuxline.vim
# on Mon, 11 Nov 2019

set -g status-justify "centre"
set -g status "on"
set -g status-left-style "none"
set -g message-command-style "fg=colour13,bg=colour11"
set -g status-right-style "none"
set -g pane-active-border-style "fg=colour2"
set -g status-style "none,bg=colour10"
set -g message-style "fg=colour13,bg=colour11"
set -g pane-border-style "fg=colour11"
set -g status-right-length "100"
set -g status-left-length "100"
setw -g window-status-activity-style "none,fg=colour2,bg=colour10"
setw -g window-status-separator ""
setw -g window-status-style "none,fg=colour9,bg=colour10"
set -g status-left "#[fg=colour10,bg=colour2] #S #[fg=colour2,bg=colour11,nobold,nounderscore,noitalics]#[fg=colour13,bg=colour11] #W #[fg=colour11,bg=colour10,nobold,nounderscore,noitalics]"
set -g status-right "#[fg=colour2,bg=colour10,nobold,nounderscore,noitalics]#[fg=colour10,bg=colour2] #H "
setw -g window-status-format "#[fg=colour10,bg=colour10,nobold,nounderscore,noitalics]#[default] #I #W #[fg=colour10,bg=colour10,nobold,nounderscore,noitalics]"
setw -g window-status-current-format "#[fg=colour10,bg=colour11,nobold,nounderscore,noitalics]#[fg=colour13,bg=colour11] #I #W #[fg=colour11,bg=colour10,nobold,nounderscore,noitalics]"
