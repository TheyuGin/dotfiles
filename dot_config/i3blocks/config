[mediaplayer]
color="#f8f8f2"
command=./scripts/mediaplayer
instance='spotify'
interval=1
signal=10

[cpu_util_detailed]
command=./scripts/cpu_util_detailed
label=CPU
markup=pango
interval=repeat

[memory]
command=./scripts/memory
label=MEM 
interval=5

[memory]
command=./scripts/memory
label=SWAP 
interval=5
instance=swap

[i3-volume]
command=i3-volume output "%i %v\n" -t i3blocks -u 10
interval=repeat
signal=10
color="#f8f8f2"
separator_block_width=10
markup=pango

[keymap]
color="#f8f8f2"
label=‚å®
command=xkb-switch && xkb-switch -W
interval=persist

[time]
color="#f8f8f2"
command=date +'%T %Y/%m/%e'
interval=1

