[weather]
command=curl -Ss 'https://wttr.in?0&T&Q' | cut -c 16- | head -2 | xargs echo
interval=3600
color=statusline


[pulsevolume]
AUDIO_HIGH_SYMBOL=
AUDIO_MED_SYMBOL=
AUDIO_MUTED_SYMBOL=
DEFAULT_COLOR=statusline
MUTED_COLOR="#44475A"

command=./scripts/pulsevolume
interval=once
signal=1
USE_ALSA_NAME=1

[mediaplayer]
color=statusline
command=./scripts/mediaplayer
instance=''
interval=1
signal=10

[keymap]
color=statusline
label=⌨
command=xkb-switch -W
interval=persist

[time]
color=statusline
command=date +'%T %e/%m/%y'
interval=1

# vim:syntax=dosini
