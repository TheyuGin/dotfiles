[mediaplayer]
color=statusline
command=./scripts/mediaplayer
instance=''
interval=1
signal=10

[pulsevolume]
AUDIO_HIGH_SYMBOL=
AUDIO_MED_SYMBOL=
AUDIO_MUTED_SYMBOL=
DEFAULT_COLOR=statusline
MUTED_COLOR="#44475A"

command=./scripts/pulsevolume
interval=once
signal=1
USE_ALSA_NAME=1

[keymap]
color=statusline
label=⌨
command=xkb-switch -W
interval=persist

[time]
color=statusline
command=date +'%T %e/%m/%y'
interval=1

