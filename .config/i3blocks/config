[weather]
command=curl -Ss 'https://wttr.in?0&T&Q' | cut -c 16- | head -2 | xargs echo
interval=3600
color=statusline


[pulsevolume]
command=./scripts/pulsevolume
interval=once
signal=1
USE_ALSA_NAME=1

[mediaplayer]
command=./scripts/mediaplayer
instance=spotify
interval=1
signal=10

